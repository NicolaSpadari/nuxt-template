{
  "name": "nuxt-template",
  "type": "module",
  "version": "4.0.0",
  "private": true,
  "packageManager": "bun@1.2.19",
  "description": "Personal Nuxt 4 starter template with UnoCSS",
  "author": "Nicola Spadari",
  "engines": {
    "node": ">=23"
  },
  "scripts": {
